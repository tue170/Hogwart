<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="housePane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.HouseController">
   <children>
     
      <Button fx:id="home" layoutX="694.0" layoutY="6.0" mnemonicParsing="false" prefHeight="48.0" prefWidth="100.0" text="Home">
         <font>
            <Font name="Arial Bold" size="17.0" />
         </font></Button>
      <Button fx:id="enrollButton" layoutX="320.0" layoutY="520.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="250.0" text="Enroll New Student">
         <font>
            <Font name="System Bold" size="17.0" />
         </font></Button>
      <Label fx:id="houseNameLabel" layoutX="225.0" layoutY="13.0" prefHeight="80.0" prefWidth="466.0">
         <font>
            <Font name="Copperplate Gothic Bold" size="48.0" />
         </font>
      </Label>
      <Label layoutX="62.0" layoutY="300.0" prefHeight="49.0" prefWidth="93.0" text="Head:">
         <font>
            <Font name="Cambria Bold" size="32.0" />
         </font>
      </Label>
      <Label fx:id="houseHeadLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="9.0" layoutY="330.0" prefHeight="68.0" prefWidth="205.0" textAlignment="CENTER">
         <font>
            <Font name="Cambria Bold" size="20.0" />
         </font></Label>
      <ImageView fx:id="houseImage" fitHeight="230.0" fitWidth="192.0" layoutX="8.0" layoutY="13.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../logos/gryffindor.jpg" />
         </image></ImageView>
      <TableView fx:id="studentTable" layoutX="220.0" layoutY="100.0" prefHeight="404.0" prefWidth="524.0">
        <columns>
          <TableColumn fx:id="studentNameCol" editable="false" prefWidth="446.0" style="-fx-text-fill: black;" text="Student Name" />
          <TableColumn fx:id="studentYearCol" editable="false" minWidth="0.0" prefWidth="77.0" style="-fx-text-fill: black;" text="Year" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
